{
  "extends": [
    "development"
  ],
  "hints": {
    "no-inline-styles": "off",
    "meta-viewport": "off",
    "axe/language": [
      "default",
      {
        "html-has-lang": "off"
      }
    ],
    "axe/text-alternatives": [
      "default",
      {
        "document-title": "off"
      }
    ],
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "meta[name=theme-color]",
          "meta[name=theme-color]",
          "meta[name=theme-color]",
          "link[fetchpriority]",
          "link[fetchpriority]",
          "link[fetchpriority]",
          "link[fetchpriority]"
        ]
      }
    ]
  }
}